<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:tns="http://schemas.datacontract.org/2004/07/Wcf_Chat" elementFormDefault="qualified" targetNamespace="http://schemas.datacontract.org/2004/07/Wcf_Chat" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:complexType name="PrivateMessage">
    <xs:sequence>
      <xs:element minOccurs="0" name="FromId" type="xs:int" />
      <xs:element minOccurs="0" name="ID" nillable="true" type="xs:string" />
      <xs:element minOccurs="0" name="Msg" nillable="true" type="xs:string" />
      <xs:element minOccurs="0" name="ToId" type="xs:int" />
      <xs:element minOccurs="0" name="dateTime" type="xs:dateTime" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="PrivateMessage" nillable="true" type="tns:PrivateMessage" />
  <xs:complexType name="ArrayOfPrivateMessage">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="PrivateMessage" nillable="true" type="tns:PrivateMessage" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="ArrayOfPrivateMessage" nillable="true" type="tns:ArrayOfPrivateMessage" />
</xs:schema>